{
  "expo": {
    "name": "Ez Sticker Smash",
    "slug": "StickerSmash",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "description": "Ez Sticker Mash is an exciting and innovative app that allows users to create their own unique sticker designs quickly and easily. With a user-friendly interface and a wide range of customization options, this app makes it fun and simple to express your creativity and design personalized stickers that are perfect for any occasion. Whether you want to make stickers for your social media posts, decorate your photos, or simply add a fun touch to your messaging conversations, Ez Sticker Mash has everything you need to bring your ideas to life. So why wait? Download the app today and start mashing up your own stickers!",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#20242A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/1ce41990-0d0d-49f6-badd-c32293a2bfca"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "appStoreUrl": "https://apps.apple.com/us/app/ez-sticker-smash/id6447040753",
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.ezerway.StickerSmash",
      "buildNumber": "22",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.ezerway.StickerSmash",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ezerway.StickerSmash",
      "versionCode": 27
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1ce41990-0d0d-49f6-badd-c32293a2bfca"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "locales": {
      "vi": "./languages/vi.json",
      "en": "./languages/en.json"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you create freely."
        }
      ],
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-updates",
        {
          "username": "ezerway"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-firebase/crashlytics"
    ]
  }
}
